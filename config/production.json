{
  "react": {
    "root": "require:./build/components#default",
    "reducers": "require:./build/reducers#default",
    "render": "path:./build/middleware/renderReact",
    "serverRenderPath": "path:./build/middleware/renderReact"
  },
  "vendor": {
    "minChunks": "require:./build/utils/isVendor#default"
  },
  "webpack": {
    "mode": "production",
    "plugins": {
      "clean": {
        "module": {
          "name": "clean-webpack-plugin",
          "arguments": [
            ["config:distPath"],
            {
              "root": "config:rootPath"
            }
          ]
        }
      },
      "loader-options": {
        "module": {
          "name": "webpack",
          "method": "LoaderOptionsPlugin",
          "arguments": [
            {
              "minimize": true,
              "debug": false
            }
          ]
        }
      }
    }
  }
}
